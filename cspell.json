{
    "version": "0.2",
    "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
    "enableGlobDot": true,
    "language": "en,es-ES,en-GB",
    "words": [
        "popfirstandpush",
        "pushreplacement"
    ],
    "useGitignore": true,
    "ignorePaths": [
        ".git/*",
        ".git/!{COMMIT_EDITMSG,EDITMSG}",
        ".git/*/**",
        ".vscode",
        ".gitignore",
        "cspell.json"
    ]
}